# OpenAI API Key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Duffel API Token (required for hotel search)
DUFFEL_API_TOKEN=your_duffel_api_token_here

# Optional: Set your organization ID if you have one
# OPENAI_ORG_ID=your_org_id_here

# Optional: Duffel API Configuration
# DUFFEL_BASE_URL=https://api.duffel.com
# DUFFEL_API_VERSION=v2

# Optional: Request timeouts and retries
# REQUEST_TIMEOUT=30
# MAX_RETRIES=3

# LangSmith tracing (optional)
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=your_langsmith_api_key_here
# LANGCHAIN_PROJECT=BookedAI-Agent 