{
  "name": "@duffel/components",
  "version": "2.7.20",
  "description": "Set of pre-built UI components for integrating with Duffel API",
  "main": "dist/duffel-components.js",
  "module": "dist/duffel-components.esm.js",
  "jsnext:main": "dist/duffel-components.esm.js",
  "unpkg": "dist/duffel-components.umd.js",
  "unpkg:min": "dist/duffel-components.umd.min.js",
  "types": "dist/duffel-components.d.ts",
  "scripts": {
    "prebuild": "node -p \"'export const DUFFEL_COMPONENTS_VERSION = ' + JSON.stringify(require('./package.json').version) + ''\" > ./src/version.ts && yarn prettier",
    "build": "webpack --mode production --config webpack.config.babel.js && yarn create-dts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "pre-commit-check": "yarn lint-staged && yarn test --passWithNoTests && yarn build && yarn e2e",
    "prepare": "husky install",
    "get-version": "echo $npm_package_version",
    "release": "npx semantic-release --debug",
    "commit": "git-cz",
    "commitlint": "commitlint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "create-dts": "dts-bundle-generator -o dist/duffel-components.d.ts src/index.ts --no-check",
    "server": "http-server -p 1234",
    "e2e": "npx playwright test",
    "e2e:debug": "PWDEBUG=1 npx playwright test",
    "prettier": "prettier --write --loglevel=warn .",
    "prettier-check": "prettier --check .",
    "local-package:update": "yarn build && yalc push --replace --changed",
    "local-package:clean": "yalc installations clean @duffel/components"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{js,jsx,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "files": [
    "dist/**"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@stripe/react-stripe-js": "1.8.0",
    "@stripe/stripe-js": "^1.35.0",
    "base64-js": "1.5.1",
    "classnames": "2.3.2",
    "decimal.js": "^10.4.3",
    "ieee754": "1.2.1",
    "prop-types": "15.8.1",
    "react-popper": "2.2.5",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-unicode-property-regex": "7.18.6",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/register": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-angular": "17.4.4",
    "@commitlint/cz-commitlint": "17.4.4",
    "@playwright/test": "1.31.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-controls": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "27.5.2",
    "@types/lodash": "4.14.195",
    "@types/node": "17.0.42",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@types/smoothscroll-polyfill": "0.3.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "babel-loader": "8.3.0",
    "babel-plugin-module-resolver": "4.1.0",
    "chromatic": "6.7.4",
    "clean-webpack-plugin": "4.0.0",
    "commitizen": "4.2.6",
    "css-loader": "6.7.3",
    "dotenv": "16.0.3",
    "dts-bundle-generator": "6.12.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "eslint-plugin-storybook": "0.6.12",
    "fork-ts-checker-webpack-plugin": "7.2.14",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "inquirer": "9.1.4",
    "jest": "26.6.3",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "2.6.1",
    "node-polyfill-webpack-plugin": "2.0.1",
    "postcss": "8.4.21",
    "postcss-combine-duplicated-selectors": "10.0.3",
    "postcss-discard-duplicates": "5.1.0",
    "postcss-import": "14.1.0",
    "postcss-loader": "7.0.2",
    "postcss-nested": "5.0.6",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "19.0.5",
    "ts-jest": "26.5.6",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "types-webpack-bundler": "1.0.2",
    "typescript": "4.7.4",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": ">=14.*"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "readme",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "breaking": true,
              "release": "major"
            },
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator"
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "resolutions": {
    "@types/webpack": "^5.28.0",
    "immer": "^9.0.6",
    "ansi-regex": "^5.0.1",
    "glob-parent": "^6.0.0",
    "browserslist": "^4.16.5",
    "trim": "^1.0.0",
    "@types/react-dom": "18.0.11",
    "@types/react": "18.0.34",
    "loader-utils": "^2.0.4"
  },
  "keywords": [
    "seat",
    "maps",
    "seat",
    "selection",
    "seat",
    "selections",
    "duffel",
    "duffelhq",
    "seat",
    "seatmap",
    "payment",
    "payments",
    "baggage",
    "baggages"
  ],
  "author": "Duffel Technology Limited <help@duffel.com> (http://duffel.com)",
  "license": "MIT"
}
